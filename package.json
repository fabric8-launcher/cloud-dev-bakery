{
  "name": "launcher-creator-frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start-and-watch": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start": "yarn start:staging-api",
    "start:mock-api": "env-cmd .env.mock-api yarn start-and-watch",
    "start:staging-api": "env-cmd .env.staging-api yarn start-and-watch",
    "start:local-api": "env-cmd .env.local-api yarn start-and-watch",
    "start:prod": "run-s build serve",
    "start:custom": "env-cmd .env.custom yarn start-and-watch",
    "doc": "npx styleguidist server",
    "build-doc": "styleguidist build",
    "deploy-doc": "yarn build-doc && gh-pages -d styleguide",
    "preinstall": "rm -f node_modules/@",
    "postinstall": "ln -s `pwd`/src node_modules/@"
  },
  "dependencies": {
    "@patternfly/react-core": "1.49.1",
    "@patternfly/react-icons": "2.10.0",
    "@types/classnames": "2.2.6",
    "@types/enzyme": "3.1.13",
    "@types/jest": "23.3.12",
    "@types/lodash": "4.14.116",
    "@types/node": "10.12.18",
    "@types/react": "16.7.20",
    "@types/react-redux": "7.0.0",
    "axios": "0.18.0",
    "classnames": "2.2.6",
    "env-cmd": "8.0.2",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "enzyme-to-json": "3.3.5",
    "gh-pages": "2.0.1",
    "jssha": "2.3.1",
    "keycloak-js": "4.3.0",
    "lodash": "4.17.11",
    "ncp": "2.0.0",
    "node-sass": "4.11.0",
    "npm-run-all": "4.1.3",
    "react": "16.7.0",
    "react-docgen-typescript": "1.12.3",
    "react-dom": "16.7.0",
    "react-redux": "6.0.0",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.3",
    "react-styleguidist": "8.0.6",
    "redux": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.2",
    "redux-saga-requests": "0.17.0",
    "redux-saga-requests-axios": "0.7.1",
    "redux-saga-requests-mock": "0.1.0",
    "regenerator-runtime": "0.12.1",
    "reselect": "3.0.1",
    "ts-optchain": "0.1.1",
    "tslint": "5.12.1",
    "tslint-react": "3.6.0",
    "typescript": "3.2.4",
    "uuid": "3.3.2"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
